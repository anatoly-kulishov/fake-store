{
  "compilerOptions": {
    "target": "esnext",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "alwaysStrict": true,
    "jsx": "preserve",
    "incremental": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "./app/",
    "paths": {
      "@/components/*": [
        "./components/*"
      ],
      "@/shared/*": [
        "shared/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/providers/*": [
        "providers/*"
      ],
      "@/services/*": [
        "services/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/assets/*": [
        "assets/*"
      ],
      "@/configs/*": [
        "configs/*"
      ],
      "@/store/*": [
        "store/*"
      ],
      "@/api/*": [
        "api/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ]
  },
  "lib": [
    "dom",
    "es2017",
    "dom.iterable",
    "esnext"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
